{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppsettingsRules": {
    "Rules": [
      {
        "Description": "PasswordMustContainMinimumCharactersRule",
        "Position": "3"
      },
      {
        "Description": "PasswordMustContainAtLeastOneDigitRule",
        "Position": "2"
      },
      {
        "Description": "PasswordMustContainAtLeastOneCapitalLetterRule",
        "Position": "1"
      },
      {
        "Description": "PasswordMustContainAtLeastOneLowercaseLetterRule",
        "Position": "4"
      },
      {
        "Description": "PasswordMustContainAtLeastOneSpecialCharacter",
        "Position": "5"
      },
      {
        "Description": "PasswordMustNotContainRepeatedCharacters",
        "Position": "6"
      }
    ]
  }
}
